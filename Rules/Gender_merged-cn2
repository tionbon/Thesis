******** Data/bonus.tab ********
Rules	Score
IF Gender==F AND Race==B AND Education==Highschool THEN Bonus=no 	0.0
IF Education==Bachelors AND Warnings==medium AND Gender==F THEN Bonus=no 	0.0
IF Education==Masters AND Warnings==high AND Gender==F THEN Bonus=no 	0.0
IF Education==Bachelors AND Warnings==high AND Gender!=F THEN Bonus=no 	0.0
IF Education==Highschool AND Race!=B AND Warnings==low THEN Bonus=no 	0.0
IF Race==B AND Warnings==low AND Education==Bachelors THEN Bonus=no 	0.0
IF Warnings==medium AND Gender!=F AND Years==3-6 THEN Bonus=no 	0.0
IF Education==Masters AND Warnings==low AND Years==>6 THEN Bonus=no 	0.0
IF Education==Masters AND Warnings==low AND Years==3-6 THEN Bonus=no 	0.0
IF Years==<3 AND Warnings==medium AND Gender!=F THEN Bonus=no 	0.0
IF Years==>6 AND Warnings==medium AND Gender!=F THEN Bonus=no 	0.0
IF Warnings==high AND Education==Bachelors AND Gender==F THEN Bonus=no 	0.0
IF Education==Masters AND Years==<3 AND Warnings==low THEN Bonus=no 	0.0
IF Education==Highschool AND Warnings==high AND Race!=B THEN Bonus=no 	0.0
IF Gender!=F AND Warnings==high AND Education==Masters THEN Bonus=no 	0.0
IF Warnings==medium AND Gender==F AND Education==Masters THEN Bonus=no 	0.0
IF Education==Highschool AND Warnings==medium AND Gender==F THEN Bonus=no 	0.0
IF Warnings==low AND Education==Bachelors AND Race!=B THEN Bonus=no 	0.0
IF Education==Highschool AND Race==B AND Warnings==low THEN Bonus=no 	0.0
IF Education==Highschool AND Warnings==high AND Race==B THEN Bonus=no 	0.0
IF Warnings==low AND Education==Bachelors AND Race!=B THEN Bonus=yes 	0.0
IF Education==Masters AND Warnings==medium AND Gender==F THEN Bonus=yes 	0.0
IF Gender!=F AND Education==Highschool AND Warnings==high THEN Bonus=yes 	0.0
IF Gender!=F AND Education==Masters AND Years==<3 THEN Bonus=yes 	0.0
IF Warnings==high AND Education==Bachelors AND Gender==F THEN Bonus=yes 	0.0
IF Years==3-6 AND Education==Masters AND Gender!=F THEN Bonus=yes 	0.0
IF Education==Highschool AND Warnings==low AND Gender!=F THEN Bonus=yes 	0.0
IF Years==>6 AND Warnings==medium AND Gender!=F THEN Bonus=yes 	0.0
IF Warnings==low AND Education==Masters AND Gender==F THEN Bonus=yes 	0.0
IF Education==Highschool AND Gender==F AND Race!=B THEN Bonus=yes 	0.0
IF Warnings==high AND Education==Masters AND Years==>6 THEN Bonus=yes 	0.0
IF Education==Bachelors AND Race==B AND Warnings==low THEN Bonus=yes 	0.0
IF Warnings==medium AND Education==Bachelors AND Years==3-6 THEN Bonus=yes 	0.0
IF Warnings==high AND Education==Bachelors AND Gender!=F THEN Bonus=yes 	0.0
IF Warnings==medium AND Years==<3 AND Education==Bachelors THEN Bonus=yes 	0.0
IF Education==Highschool AND Warnings==medium AND Gender!=F THEN Bonus=yes 	0.0
IF Race==B AND Education==Highschool AND Gender==F THEN Bonus=yes 	0.0
IF Education==Masters AND Warnings==high AND Gender==F THEN Bonus=yes 	0.0
IF Years==>6 AND Warnings!=high AND Education!=Highschool THEN Bonus=yes 	0.0
IF TRUE THEN Bonus=no 	0.0


